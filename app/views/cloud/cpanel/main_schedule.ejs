  font-family: "Rubik", sans-serif;
}

.scontainer {
  border: 1px solid rgb(105, 104, 104);
  top: 0;
  width:100% ;
  height: 100%;
  bottom: 0;
  left: 0;
  margin: auto;
  display: grid;
  place-items: center;
  background-color: #212223;
}

.items {
  width: 95%;
  background: #222222;
  box-shadow: 0 3px 6px rgba(0, 0, 0, 0.16), 0 3px 6px rgba(0, 0, 0, 0.23);
  border-top: 10px solid #0e570b;
  overflow: hidden;
  overflow-y: scroll;
