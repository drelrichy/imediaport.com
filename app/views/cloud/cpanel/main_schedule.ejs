  display: grid;
  grid-template-columns: 2fr 10fr 1fr 2fr;
  font-size: 13px;
  grid-gap: 10px;
  border: 1px solid transparent;
  cursor: pointer;
  border-radius: 15px;
  background-color: #2d2e2f;
  border: 3px solid #0e570b;


}

.items-body-content:hover {
  border-radius: 15px;
  border: 2px solid #0e570b;

}

.items-body-content i {
